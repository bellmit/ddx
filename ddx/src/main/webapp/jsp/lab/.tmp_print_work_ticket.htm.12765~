        <script type="text/javascript">
            $(document).ready(function() {
                window.print();

                $(".no_print").on("click", ".print", function(event) {
                    event.stopPropagation();
                    event.preventDefault();

                    window.print();
                });

                $('.attachments').imagesLoaded(function() {
                    $('.attachments').masonry({
                        itemSelector: 'li'
                    });
                });
            });
